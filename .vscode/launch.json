{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Django",
      "type": "python",
      "request": "launch",
      "stopOnEntry": true,
      "pythonPath": "${workspaceFolder}/env/bin/python",
      "program": "${workspaceFolder}/env/bin/invoke",
      "cwd": "${workspaceFolder}",
      "args": [
        "run.web"
      ],
      "debugOptions": [
        "RedirectOutput",
        "DjangoDebugging"
      ]
    }
  ]
}
