{
  "python.pythonPath": "${workspaceFolder}/env/bin/python",
  "python.linting.prospectorEnabled": true,
  "editor.rulers": [
    100
  ],
  "editor.tabSize": 4,
  "files.eol": "\n",
  "[javascript]": {
    "editor.tabSize": 2,
  },
  "[html]": {
    "editor.tabSize": 2,
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/__pycache__": true
  },
  "editor.wordWrap": "on",
  "files.insertFinalNewline": true,
}
