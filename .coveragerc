[run]
source = supervisr/
omit =
    env/
    manage.py
    supervisr/cli/tasks/*
    supervisr/cli/_seed/*
    supervisr/core/wsgi.py
    supervisr/*settings*.py
    supervisr/_tasks/*
    */migrations/*
    */apps.py
    docs/

[report]
sort = Cover
skip_covered = True
precision = 2
exclude_lines =
  pragma: no cover

    # Don't complain about missing debug-only code:
    def __unicode__
    def __str__
    def __repr__
    if self\.debug

    # Don't complain if tests don't hit defensive assertion code:
    raise AssertionError
    raise NotImplementedError

    # Don't complain if non-runnable code isn't run:
    if 0:
    if __name__ == .__main__.:

show_missing = True
