supervisr_mailuser:
  debug_print = "R: supervisr_mailuser for $local_part@$domain"
  driver = redirect
  domains = +supervisr_domain
  data = ${lookup mysql {SUPERVISR_MAILUSER}{$value}fail}
  directory_transport = maildir_supervisr
  no_more
